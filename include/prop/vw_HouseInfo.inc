stock HouseInfo_Load()//вызывается из OnGameModeInit
{
	new time = GetTickCount();
	if( !fexist("Property/house.info") ) { HouseInfo_Save();	return 1; }
	new File: pFile = fopen("Property/house.info", io_read);//io_readwrite
	if(pFile)
	{
		new strFromFile[256];	new arrCoords[29][MAX_PLAYER_NAME];
		for(new idx=0; idx<MAX_HOUSE; idx++)
		//while (idx < sizeof(HouseInfo))
		{
			fread(pFile, strFromFile);
			split(strFromFile, arrCoords, ',');

			HouseInfo[idx][hOwned] = strval(arrCoords[0]);
			strmid(HouseInfo[idx][hOwner], arrCoords[1], 0, strlen(arrCoords[1]), MAX_PLAYER_NAME);
			strmid(HouseInfo[idx][hDiscription], arrCoords[2], 0, strlen(arrCoords[2]), 20);
			HouseInfo[idx][hRooms] = strval(arrCoords[3]);
			
