stock object_OnGameModeInit()///загрузка глобальных объектов
{
	new time = GetTickCount();
	
	new found = 0;
	for(new o=0; o<MAX_STREAM_OBJECTS; o++)
	{
		//после модели 17005 начинаем ставить во всех интерьерах и виртмирах
		if(DynamicObject[o][oModel] == 17005 && found == 0) found = 1;
		if(found == 1)
		{
			CreateObjectEx(DynamicObject[o][oModel],DynamicObject[o][oX],DynamicObject[o][oY],DynamicObject[o][oZ],
							DynamicObject[o][orX],DynamicObject[o][orY],DynamicObject[o][orZ], -1, -1, -1, 100.0);
		}
		else
		{
			CreateObjectEx(DynamicObject[o][oModel],DynamicObject[o][oX],DynamicObject[o][oY],DynamicObject[o][oZ],
							DynamicObject[o][orX],DynamicObject[o][orY],DynamicObject[o][orZ], 0, 0, -1, 300.0);
		}
	}
	printf("9/19. [Загрузка объектов]: Было добавлено - %d, потрачено - %d (ms)", MAX_STREAM_OBJECTS, GetTickCount() - time);
}

