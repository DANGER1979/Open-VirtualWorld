public IslandTimer()
{
	for(new i = 0; i < GetMaxPlayers(); i++)
	{
		if( !IsPlayerConnected(i) || IsPlayerNPC(i) ) continue;
		PlayerObjectUpdate(i);
	}
}
stock PlayerObjectUpdate(playerid)
{
	for(new i = 0; i < sizeof(IslandObjects); i++)
	{
   		if( IsPlayerInRangeOfPoint(playerid, DrawDistance, IslandObjects[i][ox]+iX,IslandObjects[i][oy]+iY,IslandObjects[i][oz]+iZ) )
		{
			if(Player[playerid][view][i] == false)
			{
				Player[playerid][objid][i] = CreatePlayerObjectEx(playerid,
					IslandObjects[i][iModelid],
					IslandObjects[i][ox]+iX,IslandObjects[i][oy]+iY,IslandObjects[i][oz]+iZ,
					IslandObjects[i][orx],IslandObjects[i][ory],IslandObjects[i][orz],
					DrawDistance);
				Player[playerid][view][i] = true;
			}
		}
		else if( !IsPlayerInRangeOfPoint(playerid, DrawDistance, IslandObjects[i][ox]+iX,IslandObjects[i][oy]+iY,IslandObjects[i][oz]+iZ) )
		{
			if(Player[playerid][view][i] == true)
			{
				DestroyPlayerObjectEx(playerid,Player[playerid][objid][i]);
				Player[playerid][view][i] = false;
			}
		}
	}
}

stock GetXYInfrontOfPlayer(playerid, Float:distance, &Float:x, &Float:y)
{   //получить координаты перед игроком
    new Float:z, Float:angle;
 	GetPlayerPos(playerid, x, y, z);
	GetPlayerFacingAngle(playerid, angle);
    x += (distance * floatsin(-angle, degrees));
    y += (distance * floatcos(-angle, degrees));
}
