stock bus_OneSecondTimers(playerid)
{
	if( AFK_IdleTime[playerid] >= 3) return 1;
	if( PlayerInfo[playerid][pJob] == BUSDRIVER &&
		BusStopTime[playerid] > 0)//для автобусников начало
	{   //для разморозки на остановки
		BusStopTime[playerid] ++;
	    if(BusStopTime[playerid] >= 10)
		{
			BusStopTime[playerid] = 0;
			TogglePlayerControllableEx(playerid, 1);//игрок сможет двигаться
		}
	}
//для автобусников конец
	return 1;
}

stock bus_EnterRaceCheckpoint(playerid)
{
	if(PlayerInfo[playerid][pJob] != BUSDRIVER) return 1;
	if(!(nCarModel[playerid] == 431 || nCarModel[playerid] == 437))  return 1;

  	//new strings[MAX_STRING];
	if(gJobDuty[playerid] && BusRoutCounter[playerid] == BusStopsCounter[playerid])
	{   //если кол-во пройденных совпало с максимальным кол-вом, т.е. игрок финишировал
		//------------------------------------------------------------------
		new money;
		if(BusRoute[playerid] == 1 || BusRoute[playerid] == 2)//BusDriver[1] == playerid || BusDriver[2] == playerid)
		{
			PlayerInfo[playerid][pBusSkill] ++;
		}
		else PlayerInfo[playerid][pBusSkill] += 5;
		if(PlayerInfo[playerid][pBusSkill] == 50)
		{ SendClientMessage(playerid, COLOR_YELLOW, "* Ваш навык водителя автобуса повышен до 2 уровня."); }
		else if(PlayerInfo[playerid][pBusSkill] == 100)
		{ SendClientMessage(playerid, COLOR_YELLOW, "* Ваш навык водителя автобуса повышен до 3 уровня."); }
		else if(PlayerInfo[playerid][pBusSkill] == 200)
		{ SendClientMessage(playerid, COLOR_YELLOW, "* Ваш навык водителя автобуса повышен до 4 уровня."); }
		else if(PlayerInfo[playerid][pBusSkill] == 400)
		{ SendClientMessage(playerid, COLOR_YELLOW, "* Ваш навык водителя автобуса повышен до 5 уровня."); }

		new level = PlayerInfo[playerid][pBusSkill];
		new skillmoney;
		if(level < 50)	skillmoney = 100 + random(100);
		else if(level >= 50 && level < 100) skillmoney = 200 + random(100);
		else if(level >= 100 && level < 200) skillmoney = 300 + random(100);
		else if(level >= 200 && level < 400) skillmoney = 400 + random(100);
		else if(level >= 400) skillmoney = RANDMONEY5;
		money = CostRoute[playerid]*(BusStopsCounter[playerid]-BusRoutCounter[playerid]) //кол-во маркеров
		    	+ CostRoute[playerid]*StopsCounter[playerid] //кол-во остановок
				+ TransportEarning[playerid]//выручка с билетов
				+ skillmoney;//зарплата за рейс от скила
		GivePlayerMoneyB(fjob+PlayerInfo[playerid][pJob], playerid, money);
		BizInfo[0][bTill] -= money; BizInfo[0][bImport] += money;
		//----------------------------------------------------------------------
		PlaySoundForPlayer(playerid, SOUND_CHECKPOINT_AMBER);
		//Для пассажиров которые находились в автобусе до ввода команды
		for(new j=0, i; j<MaxPlayers; j++)	{
			i = PLIDs[j];
			if( !IsPlayerConnectedEx(i) ) continue;
			if(IsPlayerInAnyVehicle(i))
			{
				if(IsPlayerInVehicle(i,nCarID[playerid]) && GetPlayerState(i) == PLAYER_STATE_PASSENGER)
				{
					format(strings, sizeof(strings), "~p~~n~~n~~n~~n~~n~~n~~n~~n~~n~Bus Stop: ~w~ %s",sBus[BusRoutCounter[playerid]][BusStop]);
					GameTextForPlayerEx(i, strings, 5000, 3);
		    		SendClientMessage(i, COLOR_LIGHTBLUE, "Водитель: Конечная остановка! Пожалуйста покиньте автобус!");
					RemovePlayerFromVehicleEx(i);
				}
			}
		}
	    format(strings, sizeof(strings), "Mаршрут завершен. Выручка с продажи билетов $%d. Выручка за проезд по маршруту $%d", TransportEarning[playerid], money);
	    SendClientMessage(playerid, COLOR_LIGHTBLUE, strings);
		FixJobCounter(playerid);
	}
	else if(gJobDuty[playerid] && BusRoutCounter[playerid] != BusStopsCounter[playerid])
	{
		if(sBus[BusRoutCounter[playerid]][Type] == 2)
		{   //если тип 2 то ни чего не установливать, а просто проинформировать
			BusStopTime[playerid] = 1;

			TogglePlayerControllableEx(playerid, 0);//игрок не сможет двигаться
			StopsCounter[playerid] ++;
	    	PlaySoundForPlayer(playerid, SOUND_CHECKPOINT_GREEN);
			for(new j=0, i; j<MaxPlayers; j++)	{
				i = PLIDs[j];
				if( !IsPlayerConnectedEx(i) ) continue;
				if(IsPlayerInAnyVehicle(i))
				{
					 if(IsPlayerInVehicle(i, nCarID[playerid]))
					 {  //выводим название остановки всем пассажирам
						format(strings, sizeof(strings), "~p~~n~~n~~n~~n~~n~~n~~n~~n~~n~Bus Stop: ~w~ %s",sBus[BusRoutCounter[playerid]][BusStop]);
						GameTextForPlayerEx(i,strings,5000,3);
					 }
				}
			}
		}
		else
		{
			format(strings, sizeof(strings), "~w~%d ~y~/~r~%d", BusRoutCounter[playerid], BusStopsCounter[playerid]);
			GameTextForPlayerEx(playerid, strings, 1000, 4);
			BusRoutCounter[playerid] ++;//счётчик установки следующего чекпоинта
			SetBusCheckpoint(playerid, BusRoutCounter[playerid]);  //Sets the next checkpoint in the race
			PlaySoundForPlayer(playerid, SOUND_CHECKPOINT_AMBER);
		}
	}
	return 1;
}

stock bus_LeaveRaceCheckpoint(playerid)
{
    //для автобусников начало STEP4
    if(PlayerInfo[playerid][pJob] != BUSDRIVER) return 1;
	if(!gJobDuty[playerid]) return 1;
	if(nCarModel[playerid] != 431 && nCarModel[playerid] != 437) return 1;
	if(sBus[BusRoutCounter[playerid]][Type] != 2) return 1;
	 //если текущий тип 2 то установить следующий
	BusRoutCounter[playerid] ++;//счётчик установки следующего чекпоинта
	SetBusCheckpoint(playerid, BusRoutCounter[playerid]);  //Sets the next checkpoint in the race
	PlaySoundForPlayer(playerid, SOUND_CHECKPOINT_AMBER);
	for(new j=0, i; j<MaxPlayers; j++)	{
		i = PLIDs[j];
		if( !IsPlayerConnectedEx(i) ) continue;
		if(!IsPlayerInAnyVehicle(i)) continue;
		if(IsPlayerInVehicle(i, nCarID[playerid]))//GetPlayerState(i) == PLAYER_STATE_PASSENGER &&
		{  //сканируем всех в этом автобусе
			format(strings, sizeof(strings), "~p~~n~~n~~n~~n~~n~~n~~n~~n~~n~Next Bus Stop: ~w~ %s",sBus[BusRoutCounter[playerid]][BusStop]);
			GameTextForPlayerEx(i, strings, 5000, 3);
			//return 1;
		}
	}
	return 1;
}
stock SetBusCheckpoint(playerid, progress)
{   //ф-ия установки следующих чекпоинтов
	//где progress - кол-во пройденых игроком CP, totalchecks - общее кол-во CP на данной трасе
	if(sBus[progress][Type] == 0)// && sBus[progress-1][Type] == 0
	{	//если следующий и текущий тип = 0 то установить со стрелкой
		SetPlayerRaceCheckpoint(playerid, 0,
		sBus[progress][bX], sBus[progress][bY], sBus[progress][bZ],
		sBus[progress+1][bX], sBus[progress+1][bY], sBus[progress+1][bZ], 8.0);
	}
	else if(sBus[progress][Type] == 1)// && sBus[progress-1][Type] == 0
	{	//если следующий тип 1 то установить финишный
		SetPlayerRaceCheckpoint(playerid, 1,
		sBus[progress][bX], sBus[progress][bY], sBus[progress][bZ],
		sBus[progress][bX], sBus[progress][bY], sBus[progress][bZ], 8.0);
	}
	else if(sBus[progress][Type] == 2)// && sBus[progress-1][Type] == 0
	{	//если следующий тип 2 то установить без стрелки
		SetPlayerRaceCheckpoint(playerid, 2,
		sBus[progress][bX], sBus[progress][bY], sBus[progress][bZ],
		sBus[progress+1][bX], sBus[progress+1][bY], sBus[progress+1][bZ], 8.0);
	}
}


stock bus_OnPlayerStateChange(playerid, newstate, oldstate)
{
	#pragma unused oldstate
	if(newstate == PLAYER_STATE_ONFOOT)
	{
		if(BusPassenger[playerid])
		{   //если пассажир сидел в автобусе
			if(	IsPlayerConnected(TransportDriver[playerid])
				&& !IsPlayerNPC(TransportDriver[playerid])
				&& IsPlayerInAnyVehicle(TransportDriver[playerid]))
			{
				new vehicleid = nCarID[TransportDriver[playerid]];
				if(IsPlayerInVehicle(TransportDriver[playerid], vehicleid) &&
					GetPlayerState(TransportDriver[playerid]) == PLAYER_STATE_DRIVER &&
					gJobDuty[TransportDriver[playerid]] > 0)
				{   //если пассажир был в вашем автобусе а автобусник был за рулем
			    	format(strings, sizeof(strings), "   Пассажир %s[%d] покинул автобус !", PlayerName(playerid), playerid);//+money
			        SendClientMessage(TransportDriver[playerid], COLOR_LIGHTBLUE, strings);
				    BusPassenger[playerid] = 0; TransportDriver[playerid] = MAX_PLAYERS-1;
				}
			}
		}
	}
	else if(newstate == PLAYER_STATE_PASSENGER)
	{
		//----------------------------------------------------------------------
//для автобусников
        if(GetSeatVehiclePlayer(nCarID[playerid], 0) != INVALID_PLAYER_ID)
        {//если за рулём вашей машину кто то сидит
			new driver = GetSeatVehiclePlayer(nCarID[playerid], 0);
			if(PlayerInfo[driver][pJob] == BUSDRIVER && gJobDuty[driver])
			{
				 //если пассажир сел в автобус
		        TransportDriver[playerid] = driver;//записали ID водителя
	            BusPassenger[playerid] = 1;
				BusStopTime[playerid] = 0;
				if(GetPlayerMoneyH(playerid) > TransportValue[driver])
				{
					GivePlayerMoneyH(fjob+PlayerInfo[driver][pJob], playerid, -TransportValue[driver]);//отнимаем деньги у пассажира цену за проезд на такси
					TransportEarning[driver] += TransportValue[driver];//добавляем в полную выручку автобуса
				}
				//format(strings, sizeof(strings), "* Вы уплатили $%d Водителю Автобуса.", TransportValue[driver]);
				//SendClientMessage(playerid, COLOR_LIGHTBLUE, strings);
				format(strings, sizeof(strings), "* Пассажир %s[%d] выбрал ваш Автобус.", PlayerName(playerid), playerid);
				SendClientMessage(driver, COLOR_LIGHTBLUE, strings);
			}
		}
	}
	if(newstate != PLAYER_STATE_DRIVER) return 1;
	//new vehicleid = nCarID[playerid];//nCarID[playerid] = GetPlayerVehicleID(playerid);//считывается в player_OnPlayerStateChange(playerid, newstate, oldstate)
	new modelid = nCarModel[playerid];//считывается в player_OnPlayerStateChange(playerid, newstate, oldstate)
	bus_TakeTask(playerid, modelid);
	
	return 1;
}

stock bus_TakeTask(playerid, modelid)
{
	//ФУНКЦИЯ ПОЛУЧЕНИЯ ЗАДАНИЯ автобусника
	if(modelid == 431 || modelid == 437)//для автобусников начало STEP1
	{
		if(PlayerInfo[playerid][pJob] == BUSDRIVER)
		{
			if(BusRoutCounter[playerid] != 0)
			{   //если остановки
				SendClientMessage(playerid, COLOR_GREY, "   Вы должны закончить рейс !");
				return 1;
			}
	    	new listitems[] = "1\tLS\n2\tLV1\n3\tLV2\n4\tLV3\n5\tSF\n6\tМотель-Автошкола\n7\tМэрия-Банк\n8\tБанк-Мотель\n";
			MenuOperation[playerid] = 42;
			ShowPlayerDialogEx(playerid, BUS_DIALOG, DIALOG_STYLE_LIST, "Работодатель: Выберите маршрут", listitems, "OK", "Cancel");
   		}
		else if(!IsPermitToRider(playerid) &&
				PlayerInfo[playerid][pMember] != SAN)
		{
			//RemovePlayerFromVehicleEx(playerid);
			SendClientMessage(playerid, COLOR_GREY, "  Вы не Водитель Автобуса !");
			return 1;
		}
	}
	//для автобусников
	else if(modelid != 431 && modelid != 437 &&
		PlayerInfo[playerid][pJob] == BUSDRIVER && gJobDuty[playerid])
	{
		FixJobCounter(playerid);
		SendClientMessage(playerid, COLOR_GREY, "   Рейс аннулирован !");
	}
	//----------------------------------------------------------------------
	return 1;
}
