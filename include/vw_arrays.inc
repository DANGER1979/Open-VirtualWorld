stock arrays_SetVirtualPrice()//запускается при старте мода или скрипта
{//attractiveness - привлекательность
	for(new c=0; c<MAX_VEHICLE; c++)
	{
	    if(VehicleArray[c][SeatID] > 4)
	    {
			VehicleArray[c][Virtual_Price] = 	floatround(VehicleArray[c][Virtual_Price] +
(10.0*(
VehicleArray[c][LineSpeed]/151.0+ VehicleArray[c][MaxSpeed]/162.0 + VehicleArray[c][SeatID]/500.0 +
VehicleArray[c][GasMax]/100500.0 + 1/VehicleArray[c][fuelConsumption] +
VehicleArray[c][Speedup]/60.0 + VehicleArray[c][Braking]/15.0 +
VehicleArray[c][Controllability]/50.0 +1000.0/VehicleArray[c][Weight] + 15.0/VehicleArray[c][Harming])
*VehicleArray[c][Virtual_Price]/100.0)
											);
		}
		else
		{
			VehicleArray[c][Virtual_Price] = 	floatround(VehicleArray[c][Virtual_Price] +
(10.0*(
VehicleArray[c][LineSpeed]/151.0+ VehicleArray[c][MaxSpeed]/162.0 + VehicleArray[c][SeatID]/4.0 +
